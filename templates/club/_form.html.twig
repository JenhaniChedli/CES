<div class="ibox-content">
{{ form_start(form) }}
<div class="form-group  row">
    <label class="col-sm-2 col-form-label">Nom Club :</label>
    <div class="col-sm-10">{{ form_widget(form.Nom) }}</div>
</div>
<div class="hr-line-dashed"></div>
<div class="form-group  row">
    <label class="col-sm-2 col-form-label">Description :</label>
    <div class="col-sm-10">{{ form_widget(form.description) }}</div>
</div>
<div class="hr-line-dashed"></div>
<div class="form-group  row">
    <label class="col-sm-2 col-form-label">Logo Club :</label>
    <div class="col-sm-10">{{ form_widget(form.logo) }}</div>
</div>
<div class="hr-line-dashed"></div>
<div class="form-group  row">
    <label class="col-sm-2 col-form-label">Link d'inscription :</label>
    <div class="col-sm-10">{{ form_widget(form.linkdinscri) }}</div>
</div>
<div class="hr-line-dashed"></div>
<div class="form-group  row">
    <label class="col-sm-2 col-form-label">Date de Creation :</label>
    <div class="col-sm-10">{{ form_widget(form.datedecreation) }}</div>
</div>
<div class="hr-line-dashed"></div>

    <button class="btn">{{ button_label|default('Save' ,{'attr': {'class': 'form-control'}}) }}</button>
{{ form_end(form) }}
</div>